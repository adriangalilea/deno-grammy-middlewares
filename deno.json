{
  "name": "@adriangalilea/deno-grammy-middlewares",
  "version": "0.1.1",
  "description": "A port of grammy-middlewares for Deno users of grammY with fixes and additional functionality",
  "author": "Adrian Galilea",
  "email": "adriangalilea@gmail.com",
  "homepage": "https://adriangalilea.com",
  "repository": "https://github.com/adriangalilea/deno-grammy-middlewares",
  "tasks": {
    "check": "deno check --allow-import mod.ts",
    "fmt": "deno fmt"
  },
  "fmt": {
    "include": ["src/", "mod.ts", "deno.json"]
  },
  "exports": {
    ".": "./mod.ts"
  },
  "publish": {
    "include": [
      "README.md",
      "LICENSE",
      "mod.ts",
      "deps.ts",
      "src/**/*.ts"
    ],
    "exclude": [
      "**/*.test.ts"
    ]
  },
  "workspace": [
    "."
  ],
  "license": "MIT"
}
